pipeline {
    agent {
    label 'dev'
    }
    stages {
        stage('aws_configure') {
            steps {
                sh 'java -version'
                sh 'mvn -version'
            }
        }
        stage('gitcheckout') {
            steps {
                echo "cloning git repository"
                checkout scm
            }
        
        }
    }
}